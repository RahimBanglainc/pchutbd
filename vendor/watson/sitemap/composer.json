{
    "name": "watson/sitemap",
    "description": "Generate Google Sitemaps in Laravel",
    "keywords": ["laravel", "sitemaps"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwightwatson@studious.com.au"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/http": "^7.0",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "mockery/mockery": "~1.3"
    },
    "autoload": {
        "psr-4": {
            "Watson\\": "src/Watson"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Watson\\Sitemap\\SitemapServiceProvider"
            ],
            "aliases": {
                "Sitemap": "Watson\\Sitemap\\Facades\\Sitemap"
            }
        }
    }
}
